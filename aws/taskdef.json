{
    "executionRoleArn": "",
    "containerDefinitions": [
        {
            "name": "colour-in-backend",
            "image": "",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "colour-in-backend",
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "colour-in-backend",
            "awslogs-region": "",
            "awslogs-stream-prefix": "ecs",
            "awslogs-create-group": true
        }
    }
}